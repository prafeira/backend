                    "Autenticacao"
                ],
                "summary": "Fazer login",
                "operationId": "4b7becb0762c996662e47743a2706980",
                "requestBody": {
                    "description": "Dados de login",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "secret"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Sucesso"
                    }
                }
            }
        },
        "/api/logout": {
            "post": {
                "tags": [
                    "Autenticacao"
                ],
                "summary": "Fazer logout",
                "operationId": "b0e3cbb0dab3aed6f3ae5edfab36ec3a",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "Token de autenticação (Bearer)",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/token": {
            "get": {
                "tags": [
                    "Autenticacao"
                ],
                "summary": "Obter token de acesso",
                "description": "Retorna o token de acesso do usuário autenticado.",
                "operationId": "3c82abdb36e99bc0e456c191d7b8a9d5",
                "responses": {
                    "200": {
                        "description": "Token de acesso retornado com sucesso",
                        "content": {
